{
  "name": "@coldsurfers/cloudinary-utils",
  "version": "1.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "source": "src/",
  "files": [
    "src",
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "license": "MIT",
  "devDependencies": {
    "@coldsurfers/shared-utils": "1.0.0",
    "axios": "^1.6.3",
    "form-data": "^4.0.0",
    "fs": "^0.0.1-security",
    "release-it": "^17.0.1",
    "sha1": "^1.1.1",
    "stream": "^0.0.2",
    "typescript": "^5.3.3",
    "util": "^0.12.5"
  },
  "scripts": {
    "build": "rm -rf dist && tsc",
    "prerelease": "yarn build",
    "prerelease:rc": "yarn build",
    "release": "release-it minor --ci",
    "release:rc": "release-it --ci --preRelease=rc"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release @coldsurfers/cloudinary-utils v${version}"
    },
    "github": {
      "release": true
    },
    "hooks": {
      "after:git:release": "git reset --hard HEAD"
    }
  },
  "peerDependencies": {
    "axios": "^1.6.3",
    "form-data": "^4.0.0",
    "fs": "^0.0.1-security",
    "sha1": "^1.1.1",
    "stream": "^0.0.2",
    "util": "^0.12.5"
  },
  "dependencies": {
    "@coldsurfers/shared-utils": "1.0.0"
  }
}
